// SECTION 2: ADC to Physical Unit Conversions
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

print("\n--- SECTION 2: ADC to Physical Conversion ---")

// LM35: 10mV/Â°C, ADC 10-bit (0-1023), Vref=3.3V
function adc_to_temp_lm35(adc_value: integer) -> decimal do
    let voltage = (adc_value * 3.3) / 1023.0
    let temp_c = voltage * 100.0  // 10mV/Â°C
    return temp_c
end

// NTC Thermistor (Steinhart-Hart simplified)
function adc_to_temp_ntc(adc_value: integer, r_series: decimal, r_nominal: decimal, b_coeff: decimal) -> decimal do
    let resistance = r_series * adc_value / (1023.0 - adc_value)
    // Simplified: T = 1 / (ln(R/R0)/B + 1/T0) - 273.15
    let temp_k = 1.0 / ((1.0/298.15) + (1.0/b_coeff) * (resistance/r_nominal))
    return temp_k - 273.15
end

// LDR: Light Dependent Resistor
function adc_to_lux(adc_value: integer) -> decimal do
    // Logarithmic response: lux = 10^((1023-adc)/200)
    let lux = (1023.0 - adc_value) * (LIGHT_MAX_LUX / 1023.0)
    return lux
end

// MQ Gas Sensor
function adc_to_ppm(adc_value: integer, r_load: decimal) -> decimal do
    let rs = r_load * (1023.0 - adc_value) / adc_value
    // Rs/R0 ratio to PPM (simplified)
    let ratio = rs / 10000.0  // R0 = 10k assumed
    let ppm = 100.0 / ratio
    return ppm
end

