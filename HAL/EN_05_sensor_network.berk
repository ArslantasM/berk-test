// Packet format
// [HEADER][LENGTH][TYPE][DATA...][CRC]
let PACKET_HEADER = 0xAA
let PACKET_TYPE_SENSOR = 0x01
let PACKET_TYPE_ALARM = 0x02
let PACKET_TYPE_STATUS = 0x03

function calculate_crc8(data: array, length: integer) -> integer do
    let crc = 0
    let i = 0
    while i < length do
        crc = crc ^ data[i]
        // 8-bit CRC polynomial calculation
        i = i + 1
    end
    return crc
end

