// SECTION 6: Kalman Filter
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

print("\n--- SECTION 6: Kalman Filter ---")

print("\n   Kalman Filter for Sensor Fusion:")
print("   - Reduces measurement noise")
print("   - Estimates true value from noisy readings")
print("   - Optimal for linear systems with Gaussian noise")

// Kalman filter state
var kalman_x = 0.0     // Estimated value
var kalman_p = 1.0     // Error covariance
let kalman_q = 0.01    // Process noise
let kalman_r = 0.1     // Measurement noise

function kalman_update(measurement: decimal) -> decimal do
    // Prediction phase
    // x_pred = x (static model)
    let p_pred = kalman_p + kalman_q
    
    // Update phase
    let k = p_pred / (p_pred + kalman_r)  // Kalman gain
    kalman_x = kalman_x + k * (measurement - kalman_x)
    kalman_p = (1.0 - k) * p_pred
    
    return kalman_x
end

