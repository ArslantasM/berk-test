function calculate_mean(data: array, count: integer) -> decimal do
    let sum = 0.0
    let i = 0
    while i < count do
        sum = sum + data[i]
        i = i + 1
    end
    return sum / count
end

function calculate_min(data: array, count: integer) -> decimal do
    let min_val = data[0]
    let i = 1
    while i < count do
        if data[i] < min_val then
            min_val = data[i]
        end
        i = i + 1
    end
    return min_val
end

function calculate_max(data: array, count: integer) -> decimal do
    let max_val = data[0]
    let i = 1
    while i < count do
        if data[i] > max_val then
            max_val = data[i]
        end
        i = i + 1
    end
    return max_val
end

function calculate_stddev(data: array, count: integer, mean: decimal) -> decimal do
    let sum_sq = 0.0
    let i = 0
    while i < count do
        let diff = data[i] - mean
        sum_sq = sum_sq + (diff * diff)
        i = i + 1
    end
    let variance = sum_sq / count
    // sqrt approximation
    return variance  // Would use sqrt(variance)
end

